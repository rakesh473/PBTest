@page
@model ErrorModel
@{
    ViewBag.Title = "Error";
}

<h1 class="text-danger">Error.</h1>
<h2 class="text-danger">An error occurred while processing your request.</h2><br />

@{
    if (TempData.Peek("Message") != null)
    {
        <h3 class="text-danger">Message: @TempData.Peek("Message")</h3>
    }
}